<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.arnold.fypproject.Fragment.SampleFragment"
    android:background="#6b6bbb">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Super button"
        android:id="@+id/button_super"
        android:layout_gravity="center"
        android:onClick="goToProfilePage"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Fragment text"
        android:id="@+id/textView3"
        android:layout_marginTop="50dp"
        android:layout_gravity="center_horizontal|top" />

</FrameLayout>
